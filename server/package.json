{
  "name": "pocket",
  "version": "1.0.0",
  "description": "Aplicativo de gerenciamento de finan√ßas pessoais.",
  "main": "index.js",
  "repository": "https://github.com/ej0733/Pocket.git",
  "author": "Edison Matias",
  "license": "MIT",
  "scripts": {
    "dev": "sequelize-cli db:migrate; sequelize-cli db:seed:all; ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:test": "sequelize-cli db:migrate --env=test; sequelize-cli db:seed:all --env=test; ts-node-dev --respawn --transpile-only src/index.ts --NODE_ENV=test",
    "build": "rm -rf dist; tsc",
    "start": "sequelize-cli db:migrate; node dist/index.js",
    "docker:test:create": "docker run --name pocket_test -e POSTGRES_PASSWORD=123 -e POSTGRES_DB=pocket_test -e POSTGRES_USER=user -p 5433:5432 -d postgres",
    "docker:test:clear": "docker stop pocket_test; docker rm pocket_test",
    "test:unit": "node_modules/.bin/mocha -r ts-node/register tests/unit/**/*.spec.js --require babel-register"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.95.4",
    "mysql": "^2.18.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/chai": "^4.2.18",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/sequelize": "^4.28.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
